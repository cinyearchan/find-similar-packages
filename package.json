    "bugs": {
        "url": "https://github.com/tjx666/find-similar-packages/issues",
        "email": "ytj2713151713@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tjx666/find-similar-packages"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "bumpp": "^8.2.1"
    },
    "dependencies": {
        "@pnpm/find-workspace-packages": "^5.0.16",
